{
    "name":              "cache/redis-adapter",
    "description":       "A PSR-6 cache implementation using Redis (PhpRedis). This implementation supports tags",
    "type":              "library",
    "license":           "MIT",
    "minimum-stability": "stable",
    "keywords":
    [
        "cache",
        "psr-6",
        "phpredis",
        "redis",
        "tag"
    ],
    "homepage": "https://github.com/php-cache/redis-adapter",
    "authors":
    [
        {
            "name":     "Aaron Scherer",
            "email":    "aequasi@gmail.com",
            "homepage": "https://github.com/aequasi"
        },
        {
            "name":     "Tobias Nyholm",
            "email":    "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        }
    ],
    "require":
    {
        "php":                  "^5.5|^7.0",
        "ext-redis":            "*",
        "psr/cache":            "1.0.0",
        "cache/adapter-common": "^0.1.2",
        "cache/taggable-cache": "^0.3",
        "cache/hierarchical-cache": "^0.1"
    },
    "require-dev":
    {
        "phpunit/phpunit":         "^5.1|^4.0",
        "cache/integration-tests": "dev-master"
    },
    "provide":
    {
        "psr/cache-implementation": "1.0.0"
    },
    "autoload":
    {
        "psr-4": {
            "Cache\\Adapter\\Redis\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4": {
            "Cache\\Adapter\\Redis\\Tests\\": "tests/"
        }
    }
}
